  div.rotate {
	background-color:#442267;
	border: 1px solid black;
	transform:rotate(180deg); /*45도 회전*/
	transition: width 2s,height 2s,transform 2s; /* 2초*/
  }

  div:hover {
	transform:rotate(360deg);
	transition-delay:1s;
  }

